#! /bin/sh

../../stu.test -P dskjfhksdjfhksjdfh >list.out 2>list.err
exitcode="$?"

[ "$exitcode" = 0 ] || {
	echo >&2 '*** Exit code'
	exit 1
}

[ -r list.err ] && [ ! -s list.err ] || {
	echo >&2 '*** stderr'
	exit 1
}


[ -r list.out ] && [ ! -s list.out ] || {
	echo >&2 '*** stdout'
	exit 1
}

exit 0
