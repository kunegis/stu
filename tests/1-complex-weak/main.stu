

#
# A complex example of weak cycles. 
#

A:  A8 B8 C8 D8 E8 {
    cat A8 B8 C8 D8 E8 >A
}

#-----------

A8:  A7 B7 {
     cat A7 B7 >A8
}

B8:  A7 B7 C7 {
     cat A7 B7 C7 >B8
}

C8:  B7 C7 D7 {
     cat B7 C7 D7 >C8
}

D8:  C7 D7 E7 {
     cat C7 D7 E7 >D8
}

E8:  D7 E7 {
     cat D7 E7 >E8
}

#-----------

A7:  A6 B6 {
     cat A6 B6 >A7
}

B7:  A6 B6 C6 {
     cat A6 B6 C6 >B7
}

C7:  B6 C6 D6 {
     cat B6 C6 D6 >C7
}

D7:  C6 D6 E6 {
     cat C6 D6 E6 >D7
}

E7:  D6 E6 {
     cat D6 E6 >E7
}

#-----------

A6:  A5 B5 {
     cat A5 B5 >A6
}

B6:  A5 B5 C5 {
     cat A5 B5 C5 >B6
}

C6:  B5 C5 D5 {
     cat B5 C5 D5 >C6
}

D6:  C5 D5 E5 {
     cat C5 D5 E5 >D6
}

E6:  D5 E5 {
     cat D5 E5 >E6
}

#-----------

A5:  A4 B4 {
     cat A4 B4 >A5
}

B5:  A4 B4 C4 {
     cat A4 B4 C4 >B5
}

C5:  B4 C4 D4 {
     cat B4 C4 D4 >C5
}

D5:  C4 D4 E4 {
     cat C4 D4 E4 >D5
}

E5:  D4 E4 {
     cat D4 E4 >E5
}

#-----------------

A4: {
    echo A four >A4
}

B4: {
    echo B four >B4
}

