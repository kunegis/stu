
# A file in the future should produce a warning (during a Stu command),
# but succeed.

A: B
{
	cp B A
}

B:
{
	# Create a file with a modification time in the future 
	now="$(LC_ALL=C date)"
	year="$(echo $now | sed -e 's,^.* \([0-9][0-9]*\)$,\1,')"
	year_future="$(expr 2 + $year)"
	date="$year_future"01010700
	touch -t "$date" B
}
