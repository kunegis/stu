A:  [B] {
	N=3
	[ -e list."$N" ]       || exit 1
	[ -e list."$((N+1))" ] && exit 1
	exit 0
}

>B:
{
	for i in $(seq 1 3) ; do
		printf '%s' '['
	done
	printf list.0
	for i in $(seq 1 3) ; do
		printf '%s' ']'
	done
	echo
}

>list.$n:
{
	N=3
	if [ "$n" = "$((N - 1))" ] ; then
		echo X
	else
		echo list."$((n + 1))"
	fi
}
